<?xml version="1.0" encoding="UTF-8" ?>
<library>

<script>
<![CDATA[
var labelObj;

function loadContent(obj,parentobj,preparentobj){
	
	Debug.write("loadContent: ",obj.action,obj,parentobj,preparentobj);
	
	if (obj.action=="conf1"){
        new subsubtreeitem(obj.parent.parent._subsubcontent,{list:null,depth:3,text:'room1',open:true,isleaf:true,action:'roomconf1'});
        new subsubtreeitem(obj.parent.parent._subsubcontent,{list:null,depth:3,text:'room2',open:true,isleaf:true,action:'roomconf1'});
    } else if (obj.action=="conf2"){
        new subsubtreeitem(obj.parent.parent._subsubcontent,{list:null,depth:3,text:'room1',open:true,isleaf:true,action:'meetconf1'});
        new subsubtreeitem(obj.parent.parent._subsubcontent,{list:null,depth:3,text:'room2',open:true,isleaf:true,action:'meetconf1'});
    } else if (obj.action=="roomconf1"){
        clearStageContent();
        obj.parent.parent.clearAllSubItemLists();
        canvas.connectedroom = obj.name;
        new conference(canvas.main_content._content);
    } else if (obj.action=="meetconf1"){
        clearStageContent();
        obj.parent.parent.clearAllSubItemLists();
        canvas.connectedroom = obj.name;
        new conference(canvas.main_content._content);
    }
	
}

function clearStageContent(){
    canvas.main_content._content.clearAll();
}
function setLabelObject(labelObjRef){
	labelObj = labelObjRef;
}

function getLabelNameXMLCRM(labelid){
	for (var i=0;i<labelObj.length;i++){
		if (labelid==labelObj[i].fieldvalues_id){
			return labelObj[i].value;
		}
	}
	
}
]]>
</script>

</library>
<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.xmlcrm.app.hibernate.beans.lang.Fieldvalues"
        table="fieldvalues"
    >

        <id
            name="fieldvalues_id"
            column="fieldvalues_id"
            type="java.lang.Long"
        >
            <generator class="increment">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Fieldvalues.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="name"
            type="string"
            update="true"
            insert="true"
            column="name"
        />

        <property
            name="starttime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="starttime"
        />

        <property
            name="updatetime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="updatetime"
        />

        <property
            name="deleted"
            type="boolean"
            update="true"
            insert="true"
            column="deleted"
        />

        <set
            name="fieldlanguagesvalues"
            table="fieldlanguagesvalues"
            lazy="false"
            inverse="true"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="fieldvalues_id"
            >
            </key>

            <one-to-many
                  class="org.xmlcrm.app.hibernate.beans.lang.Fieldlanguagesvalues"
            />

        </set>

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Fieldvalues.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

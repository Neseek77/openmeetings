<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="termine_todo_user" name="org.xmlcrm.app.hibernate.beans.termine.Termine_Todo_User">
    <id column="termine_todo_user_id" name="termine_todo_user_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="owner_id" type="long" column="owner_id"/>
    <property name="priority" type="int" column="priority"/>
    <property name="todo_id" type="long" column="todo_id"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="user_id" type="long" column="user_id"/>
  </class>
</hibernate-mapping>

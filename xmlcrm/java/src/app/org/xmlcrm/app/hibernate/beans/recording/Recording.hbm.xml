<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="recording" lazy="false" name="org.xmlcrm.app.hibernate.beans.recording.Recording">
    <id column="recording_id" name="recording_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="name" type="string" column="name"/>
    <many-to-one outer-join="true" not-null="false" column="rooms" lazy="false" cascade="none" name="rooms" class="org.xmlcrm.app.hibernate.beans.rooms.Rooms"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="duration" type="long" column="duration"/>
    <property name="xmlString" type="text" column="xmlString"/>
  </class>
</hibernate-mapping>

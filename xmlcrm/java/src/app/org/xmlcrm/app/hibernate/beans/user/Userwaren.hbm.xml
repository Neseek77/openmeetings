<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="userwaren" lazy="false" name="org.xmlcrm.app.hibernate.beans.user.Userwaren">
    <id column="userwaren_id" name="userwaren_id">
      <generator class="increment"/>
    </id>
    <property name="article_id" type="long" column="article_id"/>
    <property name="comment" type="string" column="comment"/>
    <property name="liefer_id" type="int" column="liefer_id"/>
    <property name="menge" type="int" column="menge"/>
    <property name="status_id" type="long" column="status_id"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="user_id" type="long" column="user_id"/>
    <property name="zahlungs_id" type="int" column="zahlungs_id"/>
  </class>
</hibernate-mapping>

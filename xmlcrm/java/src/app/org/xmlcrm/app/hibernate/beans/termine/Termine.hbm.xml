<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="termine" name="org.xmlcrm.app.hibernate.beans.termine.Termine">
    <id column="termin_id" name="termin_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="description" type="string" column="description"/>
    <property name="endtermin" type="java.util.Date" column="endtermin"/>
    <property name="open" type="int" column="open"/>
    <property name="owner_id" type="long" column="owner_id"/>
    <property name="place" type="string" column="place"/>
    <property name="starttermin" type="java.util.Date" column="starttermin"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="status_id" type="long" column="status_id"/>
  </class>
</hibernate-mapping>

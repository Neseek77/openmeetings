<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="userlevel" lazy="false" name="org.xmlcrm.app.hibernate.beans.user.Userlevel">
    <id column="level_id" name="level_id">
      <generator class="increment"/>
    </id>
    <property name="description" type="string" column="description"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="statuscode" type="int" column="statuscode"/>
  </class>
</hibernate-mapping>

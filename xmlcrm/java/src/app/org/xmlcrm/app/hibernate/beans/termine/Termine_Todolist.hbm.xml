<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="termine_todolist" name="org.xmlcrm.app.hibernate.beans.termine.Termine_Todolist">
    <id column="termine_todolist_id" name="termine_todolist_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="description" type="string" column="description"/>
    <property name="name" type="string" column="name"/>
    <property name="status_id" type="long" column="status_id"/>
    <property name="teilnehmer" type="string" column="teilnehmer"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="org.xmlcrm.app.hibernate.beans.basic.ErrorValues"
        table="errorvalues"
    >

        <id
            name="errorValuesId"
            column="errorValuesId"
            type="java.lang.Long"
        >
            <generator class="increment">
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-ErrorValues.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <property
            name="starttime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="starttime"
        />

        <property
            name="updatetime"
            type="java.util.Date"
            update="true"
            insert="true"
            column="updatetime"
        />

        <property
            name="deleted"
            type="string"
            update="true"
            insert="true"
            column="deleted"
        />

        <many-to-one
            name="fieldvalues"
            class="org.xmlcrm.app.hibernate.beans.lang.Fieldvalues"
            cascade="none"
            outer-join="true"
            update="false"
            insert="false"
            column="fieldvalues_id"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-ErrorValues.xml
            containing the additional properties and place it in your merge dir.
        -->

    </class>

</hibernate-mapping>

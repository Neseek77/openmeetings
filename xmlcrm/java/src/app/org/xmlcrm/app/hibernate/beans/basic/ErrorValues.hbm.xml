<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="errorvalues" name="org.xmlcrm.app.hibernate.beans.basic.ErrorValues">
    <id column="errorValuesId" name="errorValuesId">
      <generator class="increment"/>
    </id>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <many-to-one outer-join="true" insert="false" column="fieldvalues_id" lazy="false" update="false" name="fieldvalues" class="org.xmlcrm.app.hibernate.beans.lang.Fieldvalues"/>
    <many-to-one outer-join="true" insert="false" column="errortype_id" lazy="false" update="false" name="errorType" class="org.xmlcrm.app.hibernate.beans.basic.ErrorType"/>
  </class>
</hibernate-mapping>

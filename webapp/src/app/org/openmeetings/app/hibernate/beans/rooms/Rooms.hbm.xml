<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="rooms" lazy="false" name="org.openmeetings.app.hibernate.beans.rooms.Rooms">
    <id column="rooms_id" name="rooms_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="name" type="string" column="name"/>
    <many-to-one outer-join="true" not-null="false" column="roomtypes_id" lazy="false" cascade="none" name="roomtype" class="org.openmeetings.app.hibernate.beans.rooms.RoomTypes"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="ispublic" type="boolean" column="ispublic"/>
    <property name="numberOfPartizipants" type="long" column="numberOfPartizipants"/>
    <property name="videoPodWidth" type="integer" column="videopodwidth"/>
    <property name="videoPodHeight" type="integer" column="videopodheight"/>
    <property name="videoPodXPosition" type="integer" column="videopodxposition"/>
    <property name="videoPodYPosition" type="integer" column="videopodyposition"/>
    <property name="moderationPanelXPosition" type="integer" column="moderationpanelxposition"/>
    <property name="showWhiteBoard" type="boolean" column="showwhiteboard"/>
    <property name="whiteBoardPanelXPosition" type="integer" column="whiteboardpanelxposition"/>
    <property name="whiteBoardPanelYPosition" type="integer" column="whiteboardpanelyposition"/>
    <property name="whiteBoardPanelHeight" type="integer" column="whiteboardpanelheight"/>
    <property name="whiteBoardPanelWidth" type="integer" column="whiteboardpanelwidth"/>
    <property name="showFilesPanel" type="boolean" column="showfilespanel"/>
    <property name="filesPanelXPosition" type="integer" column="filespanelxposition"/>
    <property name="filesPanelYPosition" type="integer" column="filespanelyposition"/>
    <property name="filesPanelHeight" type="integer" column="filespanelheight"/>
    <property name="filesPanelWidth" type="integer" column="filespanelwidth"/>
    <property name="appointment" type="boolean" column="appointMent"/>
    <property name="isDemoRoom" type="boolean" column="isdemoroom"/>
    <property name="demoTime" type="integer" column="demo_time"/>
  </class>
</hibernate-mapping>

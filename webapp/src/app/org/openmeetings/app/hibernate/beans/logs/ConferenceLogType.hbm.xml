<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="conferencelogtype" name="org.openmeetings.app.hibernate.beans.logs.ConferenceLogType">
    <id column="conferencelogtype_id" name="conferenceLogTypeId">
      <generator class="increment"/>
    </id>
    <property name="eventType" type="string" column="eventtype"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="insertedby" type="long" column="insertedby"/>
  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="adresses_emails" lazy="false" name="org.openmeetings.app.hibernate.beans.adresses.Adresses_Emails">
    <id column="adresses_emails_id" name="adresses_emails_id">
      <generator class="increment"/>
    </id>
    <property name="adresses_id" type="long" column="adresses_id"/>
    <many-to-one outer-join="true" insert="true" column="mail_id" lazy="false" update="true" name="mail" class="org.openmeetings.app.hibernate.beans.adresses.Emails"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="fieldvalues" name="org.xmlcrm.app.hibernate.beans.lang.Fieldvalues">
    <id column="fieldvalues_id" name="fieldvalues_id">
      <generator class="increment"/>
    </id>
    <property name="name" type="string" column="name"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <set table="fieldlanguagesvalues" lazy="false" inverse="true" cascade="none" name="fieldlanguagesvalues">
      <key column="fieldvalues_id"/>
      <one-to-many class="org.xmlcrm.app.hibernate.beans.lang.Fieldlanguagesvalues"/>
    </set>
  </class>
</hibernate-mapping>

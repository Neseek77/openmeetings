<service name="UserService">
	<!-- Axis Description -->
    <description>
        UserService, to manage Users and Session via SOAP
    </description>
    <messageReceivers>
        <messageReceiver 
            mep="http://www.w3.org/2004/08/wsdl/in-only"
			class="org.apache.axis2.rpc.receivers.RPCInOnlyMessageReceiver"/>
        <messageReceiver
            mep="http://www.w3.org/2004/08/wsdl/in-out"
			class="org.apache.axis2.rpc.receivers.RPCMessageReceiver"/>
    </messageReceivers>		
    <parameter name="ServiceClass">org.openmeetings.axis.services.UserService</parameter>
</service>
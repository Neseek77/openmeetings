<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="terminegroups" name="org.xmlcrm.app.hibernate.beans.termine.Terminegroups">
    <id column="terminegroup_id" name="terminegroup_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="endtermin" type="java.util.Date" column="endtermin"/>
    <property name="invitor_id" type="long" column="invitor_id"/>
    <property name="message" type="string" column="message"/>
    <property name="starttermin" type="java.util.Date" column="starttermin"/>
    <property name="status_id" type="long" column="status_id"/>
    <property name="termin_id" type="long" column="termin_id"/>
    <property name="uid" type="long" column="uid"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
  </class>
</hibernate-mapping>

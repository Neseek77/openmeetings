<?xml version="1.0" encoding="UTF-8" ?>
<library>

    <class name="resetcombobox" extends="combobox">
    
        <method name="reset" >
            <![CDATA[
                var currentCount = this.cblist.getNumItems();
                for (var i = 0; i < currentCount; i++)
                    this.removeItemAt(0);
                this.resetItemsListNumber.sendEvent();
            ]]>
        </method>
    
    </class>
        
</library>

<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="rooms_moderator" lazy="false" name="org.openmeetings.app.hibernate.beans.rooms.RoomModerators">
    <id column="room_moderators_id" name="roomModeratorsId">
      <generator class="increment"/>
    </id>
    <property name="isSuperModerator" type="boolean" column="is_supermoderator"/>
    <many-to-one outer-join="true" not-null="false" column="user_id" lazy="false" cascade="none" name="user" class="org.openmeetings.app.hibernate.beans.user.Users"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="roomId" type="long" column="roomId"/>
  </class>
</hibernate-mapping>

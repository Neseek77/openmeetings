<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="userdata" lazy="false" name="org.openmeetings.app.hibernate.beans.user.Userdata">
    <id column="data_id" name="data_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment_field"/>
    <property name="data" type="string" column="data"/>
    <property name="data_key" type="string" column="data_key"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="user_id" type="long" column="user_id"/>
  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="termine_participant" name="org.xmlcrm.app.hibernate.beans.termine.Termine_Participant">
    <id column="termine_participant_id" name="termine_participant_id">
      <generator class="increment"/>
    </id>
    <property name="termin_id" type="long" column="termin_id"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="user_id" type="long" column="user_id"/>
  </class>
</hibernate-mapping>

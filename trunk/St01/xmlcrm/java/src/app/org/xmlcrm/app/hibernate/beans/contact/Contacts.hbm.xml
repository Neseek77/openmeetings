<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="contacts" name="org.xmlcrm.app.hibernate.beans.contact.Contacts">
    <id column="contact_id" name="contact_id">
      <generator class="increment"/>
    </id>
    <property name="adresses_id" type="long" column="adresses_id"/>
    <property name="age" type="long" column="age"/>
    <property name="comment" type="string" column="comment"/>
    <property name="firstname" type="string" column="firstname"/>
    <property name="freigabe_id" type="long" column="freigabe_id"/>
    <property name="lastname" type="string" column="lastname"/>
    <property name="titel_id" type="long" column="titel_id"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
  </class>
</hibernate-mapping>

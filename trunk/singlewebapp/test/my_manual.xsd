		<xs:attribute name="clip" type="xs:string" />
		<xs:attribute name="clickregion" type="xs:string" />
		<xs:attribute name="clickable" type="xs:string" />
		<xs:attribute name="capabilities" type="xs:string" />
		<xs:attribute name="cachebitmap" type="xs:string" />
		<xs:attribute name="bgcolor" type="xs:string" />
		<xs:attribute name="backgroundrepeat" type="xs:string" />
		<xs:attribute name="align" type="xs:string" />
		<xs:attribute name="aatabindex" type="xs:string" />
		<xs:attribute name="aasilent" type="xs:string" />
		<xs:attribute name="aaname" type="xs:string" />
		<xs:attribute name="aadescription" type="xs:string" />
		<xs:attribute name="aaactive" type="xs:string" />
	</xs:complexType>

	<xs:group name="topLevelElements">
		<xs:sequence>
			<xs:choice minOccurs="0" maxOccurs="unbounded">
				<xs:element ref="lzx:view" />
				<xs:element ref="lzx:myview" />
			</xs:choice>
		</xs:sequence>
	</xs:group>

</xs:schema>    
<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="usergroups" lazy="false" name="org.openmeetings.app.hibernate.beans.user.Usergroups">
    <id column="usergroup_id" name="usergroup_id">
      <generator class="increment"/>
    </id>
    <property name="level_id" type="long" column="level_id"/>
    <property name="name" type="string" column="name"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="comment" type="string" column="comment_field"/>
    <property name="description" type="string" column="description"/>
    <property name="user_id" type="long" column="user_id"/>
  </class>
</hibernate-mapping>

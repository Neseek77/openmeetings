<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="user_sip_data" lazy="false" name="org.openmeetings.app.hibernate.beans.user.UserSipData">
    <id column="user_sip_data_id" name="userSipDataId">
      <generator class="increment"/>
    </id>
    <property name="username" type="string" column="username"/>
    <property name="userpass" type="string" column="userpass"/>
    <property name="authId" type="string" column="authId"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="updated" type="java.util.Date" column="updated"/>
  </class>
</hibernate-mapping>

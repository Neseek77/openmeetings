<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="flvrecording_meta_delta" lazy="false" name="org.openmeetings.app.hibernate.beans.flvrecord.FlvRecordingMetaDelta">
    <id column="flvrecording_meta_delta_id" name="flvRecordingMetaDeltaId">
      <generator class="increment"/>
    </id>
    <property name="flvRecordingMetaDataId" type="long" column="flvrecording_metadata_id"/>
    <property name="timeStamp" type="int" column="time_stamp"/>
    <property name="deltaTime" type="long" column="delta_time"/>
    <property name="isEndPadding" type="boolean" column="is_end_padding"/>
    <property name="waveOutPutName" type="string" column="wave_out_put_name"/>
    <property name="dataLengthPacket" type="int" column="data_length_packet"/>
    <property name="receivedAudioDataLength" type="long" column="received_audio_data_length"/>
    <property name="startTime" type="java.util.Date" column="start_time"/>
    <property name="currentTime" type="java.util.Date" column="current_event_time"/>
  </class>
</hibernate-mapping>

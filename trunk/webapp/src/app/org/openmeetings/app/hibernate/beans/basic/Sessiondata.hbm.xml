<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="sessiondata" name="org.openmeetings.app.hibernate.beans.basic.Sessiondata">
    <id column="id" name="id">
      <generator class="increment"/>
    </id>
    <property name="refresh_time" type="java.util.Date" column="refresh_time"/>
    <property name="session_id" type="string" column="session_id"/>
    <property name="starttermin_time" type="java.util.Date" column="starttermin_time"/>
    <property name="user_id" type="long" column="user_id"/>
    <property name="sessionXml" type="text" column="sessionXml"/>
  </class>
</hibernate-mapping>

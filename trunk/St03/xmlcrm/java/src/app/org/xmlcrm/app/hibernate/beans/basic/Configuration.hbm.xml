<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="configuration" name="org.xmlcrm.app.hibernate.beans.basic.Configuration">
    <id column="configuration_id" name="configuration_id">
      <generator class="increment"/>
    </id>
    <property name="comment" type="string" column="comment"/>
    <property name="conf_key" type="string" column="conf_key"/>
    <property name="conf_value" type="string" column="conf_value"/>
    <property name="starttime" type="java.util.Date" column="starttime"/>
    <property name="updatetime" type="java.util.Date" column="updatetime"/>
    <property name="deleted" type="string" column="deleted"/>
    <property name="user_id" type="long" column="user_id"/>
  </class>
</hibernate-mapping>

<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="private_messages_folder" lazy="false" name="org.openmeetings.app.hibernate.beans.user.PrivateMessageFolder">
    <id column="private_message_folder_id" name="privateMessageFolderId">
      <generator class="increment"/>
    </id>
    <property name="folderName" type="string" column="folder_name"/>
    <property name="userId" type="long" column="user_id"/>
    <property name="inserted" type="java.util.Date" column="inserted"/>
    <property name="updated" type="java.util.Date" column="updated"/>
  </class>
</hibernate-mapping>

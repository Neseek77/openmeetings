        <simpleLabelButton labelid="769" width="196" x="$once{ parent.width-200 }" 
               y="$once{ parent.height-80 }" height="78" 
               onclick="this.parent.storeSettings();">
            <view resource="test_setup_start_conference_rsc" x="2" valign="middle" />
